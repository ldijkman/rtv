.text-button{border-top-color:rgba(230, 230, 230, 0.2);border-right-color:rgba(230, 230, 230, 0.2);border-bottom-color:rgba(230, 230, 230, 0.2);border-left-color:rgba(230, 230, 230, 0.2);color:var(--accent-fg-color);background-color:rgb(36, 36, 36);}
.text-button:not(:disabled):focus, .text-button:not(:disabled):hover, .text-button:not(:disabled):active{background-color:var(--toolbar-bg-color);box-shadow:rgba(230, 230, 230, 0.1) 0px 1px 2px;}
.text-button:not(:disabled):active{background-color:rgb(42, 42, 42);}
.text-button:not(:disabled):focus{box-shadow:rgba(230, 230, 230, 0.1) 0px 1px 2px, rgba(11, 78, 189, 0.4) 0px 0px 0px 2px;}
